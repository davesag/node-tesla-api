{
  "name": "node-tesla-api",
  "version": "0.1.1",
  "description": "A modern NodeJS implementation of the (unofficial) Tesla API",
  "author": "Dave Sag <davesag@gmail.com>",
  "type": "commonjs",
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/davesag"
  },
  "main": "src/index.js",
  "engines": {
    "node": ">= 8.10.0"
  },
  "files": [
    "src",
    "CONTRIBUTING.md"
  ],
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "eslint-check": "eslint --print-config src/index.js | eslint-config-prettier-check",
    "lint": "eslint .",
    "prettier": "prettier --write '**/*.{js,json,md}'",
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test mocha ./test/unit/ --require ./test/unitTestHelper.js --recursive",
    "test:unit:cov": "NODE_ENV=test nyc mocha ./test/unit/ --require ./test/unitTestHelper.js --recursive",
    "test:snyk": "snyk test",
    "snyk-protect": "snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davesag/node-tesla-api.git"
  },
  "keywords": [
    "tesla",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/davesag/node-tesla-api/issues"
  },
  "homepage": "https://github.com/davesag/node-tesla-api#readme",
  "devDependencies": {
    "@snyk/protect": "^1.981.0",
    "ajv": "^8.11.0",
    "axios": "^1.6.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "proxyquire": "^2.1.3",
    "sinon": "^15.0.0",
    "sinon-chai": "^3.5.0"
  },
  "peerDependencies": {
    "axios": ">= 0.25.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "proseWrap": "never",
    "arrowParens": "avoid",
    "trailingComma": "none",
    "printWidth": 100
  },
  "lint-staged": {
    "**/*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 87,
    "statements": 87,
    "functions": 77,
    "branches": 94,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/index.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "cache": true
  },
  "snyk": true,
  "dependencies": {
    "os-locale": "^6.0.2"
  }
}
